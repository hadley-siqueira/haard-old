    var pa : Point[5]
    var arr : Point3[10]
    var p4 : Point4
    var p5 : Point5
    var p6 : Point6
    var p7 : Point7
    var p8 : Point8
    var n : Node
    var t : Tree

    println("p size is {sizeof(p)}")
