import std.io
import std.string

class Point:
    w : char
    k : char
    x : int
    y : int
    z : int
    kk : char
    a  : long
    aa : char

    def init : void
        pass

class Point2(Point):
    foo : int
    bar : char

class Point3:
    p0 : Point
    p1 : Point2

class Point4:
    x : int
    k : char

    @virtual
    def calculate : int
        return x

class Point5(Point4):
    pass

class Point6(Point4):
    z : int

class Point7(Point6):
    ww : int

class Point8(Point7):
    p3 : Point3[5]
    ptr : int*

    @virtual
    def calculate : int
        return x

class Node:
    data  : int
    left  : Node*
    right : Node*

class Tree:
    root : Node*
    height : long

def main : void
    var p : Point
    var p2 : Point2
