def main : void
    for i = 0; i < 10; ++i:
        print(i)

    for i = 0; i < 10:
        print(i)
        ++i # FIX: i++ bugs

    for ; i < 10; ++i:
        print(i)

    for ;; ++i:
        print(i)

    for i = 0; ; ++i:
        print(i)

    for i in 0..10:
        print(i)

    for i in ['a', 'b', 'c']:
        print(i)

    if i not in [1, 2, 3]:
        print('not here')

    while true:
        print(i)
    i = 0
