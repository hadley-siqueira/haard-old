def println : void
    @n : int

    p = &n
    p = 0x10
    *p = n
    p = 0x11
    *p = '\n'

def read_int : int
    a = 0
    p = &a

    p = 0x10
    return *p

def sum : int
    @a : int
    @b : int

    c = a + b
    return c

def main : void
    a = 2
    b = 3
    a = read_int()
    d = sum(a, b)
    println(d)
